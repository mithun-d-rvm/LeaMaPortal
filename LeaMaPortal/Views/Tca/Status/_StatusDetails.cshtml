        <div class="text-uppercase popup-heading">
            Status of Tenancy Contract Agreement
        </div>
        @*<div class="p-t-5-p"> &nbsp; </div>*@
        @*<div class="row">
            <div class="col-md-3">
                <select class="master-form-input form-control">
                    <option> Select Region </option>
                    <option> Agreement 2 </option>
                    <option> Agreement 3 </option>
                </select>
            </div>
            <div class="col-md-3">
                <select class="master-form-input form-control">
                    <option> Select Property </option>
                    <option> Agreement 2 </option>
                    <option> Agreement 3 </option>
                </select>
            </div>
            <div class="col-md-3">
                <select class="master-form-input form-control">
                    <option> Unit </option>
                    <option> Agreement 2 </option>
                    <option> Agreement 3 </option>
                </select>
            </div>
            <div class="col-md-3">
                <select class="master-form-input form-control">
                    <option> Individual </option>
                    <option> Agreement 2 </option>
                    <option> Agreement 3 </option>
                </select>
            </div>
        </div>*@
        <div class="p-t-5-p"> &nbsp; </div>
        <div class="row">
            <div class="col-md-6">
                <div class="col-md-12 p-b-7">
                    <div class="col-md-6">
                        Contract Agreement No:
                    </div>
                    <div class="col-md-6">
                        @Html.HiddenFor(m=>m.Id)
                        @Html.HiddenFor(m=>m.Agreement_No)
                       <span>@Model.Agreement_No</span>
                        @*<select class="master-form-input form-control">
                            <option> Agreement 1 </option>
                            <option> Agreement 2 </option>
                            <option> Agreement 3 </option>
                        </select>*@
                    </div>
                </div>
                <div class="col-md-12 p-b-7">
                    <div class="col-md-6">
                        Contract Agreement Date:
                    </div>
                    <div class="col-md-6">
                        <span>@Model.AgreementDate</span>
                    </div>
                </div>
                <div class="col-md-12 p-b-7">
                    <div class="col-md-6">
                        Contract Tenant Type:
                    </div>
                    <div class="col-md-6">
                        <span>@Model.Tenant_Type</span>
                        @*<input type="text" class="master-form-input form-control" />*@
                    </div>
                </div>
                <div class="col-md-12 p-b-7">
                    <div class="col-md-6">
                        Tenant ID:
                    </div>
                    <div class="col-md-6">
                        @Html.HiddenFor(m=>m.Ag_Tenant_id)
                        <span>@Model.Ag_Tenant_id</span>
                        @*<input type="text" class="master-form-input form-control" />*@
                    </div>
                </div>
                <div class="col-md-12 p-b-7">
                    <div class="col-md-6">
                        Tenant Name:
                    </div>
                    <div class="col-md-6">
                        @Html.HiddenFor(m => m.Ag_Tenant_Name)
                        <span>@Model.Ag_Tenant_Name</span>
                        @*<input type="text" class="master-form-input form-control" />*@
                    </div>
                </div>
                <div class="col-md-12 p-b-7">
                    <div class="col-md-6">
                        Property ID:
                    </div>
                    <div class="col-md-6">
                        @Html.HiddenFor(m => m.Properties_ID)
                        <span>@Model.Properties_ID</span>
                        @*<input type="text" class="master-form-input form-control" />*@
                    </div>
                </div>
                <div class="col-md-12 p-b-7">
                    <div class="col-md-6">
                        Property Name:
                    </div>
                    <div class="col-md-6">
                        @Html.HiddenFor(m => m.Properties_Name)
                        <span>@Model.Properties_Name</span>
                        @*<input type="text" class="master-form-input form-control" />*@
                    </div>
                </div>
            </div>



            <div class="col-md-6">
                <div class="col-md-12 p-b-7">
                    <div class="col-md-6">
                        Unit ID:
                    </div>
                    <div class="col-md-6">
                      
                        <span>@Model.Unit_ID_Tawtheeq</span>
                        @*<input type="text" class="master-form-input form-control" />*@
                    </div>
                </div>
                <div class="col-md-12 p-b-7">
                    <div class="col-md-6">
                        Unit Name:
                    </div>
                    <div class="col-md-6">
                      
                        <span>@Model.Unit_Property_Name</span>
                        @*<input type="text" class="master-form-input form-control" />*@
                    </div>
                </div>
                <div class="col-md-12 p-b-7">
                    <div class="col-md-6">
                        Renewal Status:
                    </div>
                    <div class="col-md-6">
                        @Html.DropDownList("Renewal_Close_Flag", null, "-- Select --", new { @class = "master-form-input form-control", @required = "required" })
                        @*@Html.DropDownList("Renewal_Close_Flag", null, new { @class = "master-form-input form-control" })*@
                        @*<select class="master-form-input form-control">
                            <option> Payment 1 </option>
                            <option> Payment 2 </option>
                            <option> Payment 3 </option>
                        </select>*@
                    </div>
                </div>
                <div class="col-md-12 p-b-7">
                    <div class="col-md-6">
                        Caretaker Id:
                    </div>
                    <div class="col-md-6">
                        @Html.HiddenFor(m => m.Caretaker_id)
                        <span>@Model.Caretaker_id</span>
                    </div>
                </div>
                <div class="col-md-12 p-b-7">
                    <div class="col-md-6">
                        Caretaker Name:
                    </div>
                    <div class="col-md-6">
                        @Html.HiddenFor(m => m.Caretaker_Name)
                        <span>@Model.Caretaker_Name</span>
                    </div>
                </div>
                <div class="col-md-12 form-input-padding">
                    <div class="col-md-6">
                        &nbsp;
                    </div>
                    <div class="col-md-4">
                        <input type="submit" class="btn master-form-btn" value="Submit" />
                    </div>
                </div>
            </div>
        </div>
        <div class="p-b-10-p">
            &nbsp;
        </div>
    </div>
    @*<div class="modal-footer">
            <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
