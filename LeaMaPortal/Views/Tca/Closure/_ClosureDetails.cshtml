<div class="modal-content">
    @*<div class="modal-header">
        <button type="button" class="close" data-dismiss="modal">&times;</button>
    </div>*@
    <div class="modal-body font-smaller">
        <button type="button" class="close font-40" data-dismiss="modal">&times;</button>
        <div class="text-uppercase popup-heading">
            Contract Tenancy Agreement - Closure 
        </div>
        <div class="p-t-5-p"> &nbsp; </div>
        <div class="row">
            <div class="col-md-6">
                <div class="col-md-12 p-b-7">
                    <div class="col-md-6">
                        Contract Agreement No:
                    </div>
                    <div class="col-md-6">
                        <input type="text" class="master-form-input form-control" />
                    </div>
                </div>
                <div class="col-md-12 p-b-7">
                    <div class="col-md-6">
                        Contract Agreement Date:
                    </div>
                    <div class="col-md-6">
                        <input type="text" class="master-form-input form-control" />
                    </div>
                </div>
                <div class="col-md-12 p-b-7">
                    <div class="col-md-6">
                        Contract Tenant Type:
                    </div>
                    <div class="col-md-6">
                        <input type="text" class="master-form-input form-control" />
                    </div>
                </div>
                <div class="col-md-12 p-b-7">
                    <div class="col-md-6">
                        Tenant ID:
                    </div>
                    <div class="col-md-6">
                        <input type="text" class="master-form-input form-control" />
                    </div>
                </div>
                <div class="col-md-12 p-b-7">
                    <div class="col-md-6">
                        Tenant Name:
                    </div>
                    <div class="col-md-6">
                        <input type="text" class="master-form-input form-control" />
                    </div>
                </div>
                <div class="col-md-12 p-b-7">
                    <div class="col-md-6">
                        Property ID:
                    </div>
                    <div class="col-md-6">
                        <input type="text" class="master-form-input form-control" />
                    </div>
                </div>
                <div class="col-md-12 p-b-7">
                    <div class="col-md-6">
                        Property Tawtheeq ID:
                    </div>
                    <div class="col-md-6">
                        <input type="text" class="master-form-input form-control" />
                    </div>
                </div>
                <div class="col-md-12 p-b-7">
                    <div class="col-md-6">
                        Property Name:
                    </div>
                    <div class="col-md-6">
                        <input type="text" class="master-form-input form-control" />
                    </div>
                </div>
                <div class="col-md-12 p-b-7">
                    <div class="col-md-6">
                        Unit Tawtheeq ID:
                    </div>
                    <div class="col-md-6">
                        <input type="text" class="master-form-input form-control" />
                    </div>
                </div>
                <div class="col-md-12 p-b-7">
                    <div class="col-md-6">
                        Unit Name:
                    </div>
                    <div class="col-md-6">
                        <input type="text" class="master-form-input form-control" />
                    </div>
                </div>
            </div>



            <div class="col-md-6">
                <div class="col-md-12 p-b-7">
                    <div class="col-md-6">
                        Contract Agreement Start Date:
                    </div>
                    <div class="col-md-6">
                        <input type="text" class="master-form-input form-control" />
                    </div>
                </div>
                <div class="col-md-12 p-b-7">
                    <div class="col-md-6">
                        Contract Agreement End Date:
                    </div>
                    <div class="col-md-6">
                        <input type="text" class="master-form-input form-control" />
                    </div>
                </div>
                <div class="col-md-12 p-b-7">
                    <div class="col-md-6">
                        Contract Total Amount:
                    </div>
                    <div class="col-md-6">
                        <input type="text" class="master-form-input form-control" />
                    </div>
                </div>
                <div class="col-md-12 p-b-7">
                    <div class="col-md-6">
                        Total Amount Paid:
                    </div>
                    <div class="col-md-6">
                        <input type="text" class="master-form-input form-control" />
                    </div>
                </div>
                <div class="col-md-12 p-b-7">
                    <div class="col-md-6">
                        Amount Pending from Tenant:
                    </div>
                    <div class="col-md-6">
                        <input type="text" class="master-form-input form-control" />
                    </div>
                </div>
                <div class="col-md-12 p-b-7">
                    <div class="col-md-6">
                        Security Deposit Paid:
                    </div>
                    <div class="col-md-6">
                        <input type="text" class="master-form-input form-control" />
                    </div>
                </div>
                <div class="col-md-12 p-b-7">
                    <div class="col-md-6">
                        Less Damages (if any):
                    </div>
                    <div class="col-md-6">
                        <select class="master-form-input form-control">
                            <option> Damage 1 </option>
                            <option> Damage 2 </option>
                            <option> Damage 3 </option>
                        </select>
                    </div>
                </div>
                <div class="col-md-12 p-b-7">
                    <div class="col-md-6">
                        Total Amount to be refunded:
                    </div>
                    <div class="col-md-6">
                        <input type="text" class="master-form-input form-control" />
                    </div>
                </div>
                <div class="col-md-12 p-b-7">
                    <div class="col-md-6">
                        Remarks:
                    </div>
                    <div class="col-md-6">
                        <textarea rows="3" class="master-form-input form-control"></textarea>
                    </div>
                </div>
                <div class="col-md-12 form-input-padding">
                    <div class="col-md-6">
                        &nbsp;
                    </div>
                    <div class="col-md-4">
                        <input type="button" class="btn master-form-btn" value="Submit" />
                    </div>
                </div>
            </div>
        </div>
        <div class="p-b-10-p">
            &nbsp;
        </div>
        <!-- BEGIN AGREEMENT PDC -->
        <div class="p-lr-20">
            @Html.Partial("_AgreementPdc")
        </div>
        <!-- END AGREEMENT PDC -->
        <div class="row ht-5">
            &nbsp;
        </div>
        <!-- BEGIN AGREEMENT DOCUMENT -->
        <div class="p-lr-20">
            @Html.Partial("_AgreementDocument")
        </div>
        <!-- END AGREEMENT DOCUMENT -->
        <div class="row ht-5">
            &nbsp;
        </div>
        <!-- BEGIN AGREEMENT UTILITY -->
        <div class="p-lr-20">
            @Html.Partial("_AgreementUtility")
        </div>
        <!-- END AGREEMENT UTILITY -->
        <div class="row ht-5">
            &nbsp;
        </div>
        <!-- BEGIN AGREEMENT UNIT -->
        <div class="p-lr-20">
            @Html.Partial("_AgreementUnit")
        </div>
        <!-- END AGREEMENT UNIT -->
        <div class="row ht-5">
            &nbsp;
        </div>
        <!-- BEGIN AGREEMENT UNIT -->
        <div class="p-lr-20">
            @Html.Partial("_AgreementCheckList")
        </div>
        <!-- END AGREEMENT UNIT -->
        <div class="p-b-10-p">
            &nbsp;
        </div>
    </div>
    @*<div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
    </div>
    <div class="modal-footer">
        <button type="button" class="btn btn-default" id="test-modal">Test</button>
    </div>*@
</div>

