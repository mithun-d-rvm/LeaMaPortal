                        // $("#").addClass("hide");
                    }

                });
        }
        // $("#Title").val($("#TitleDisplay").val());
    });
    $(document).ready(function () {
        $('#Status_unit').attr('disabled', 'disabled');
        $('#Status').attr('disabled', 'disabled');
        if ($("#unit")[0].checked) {
            $("#unit").prop('checked', true).change();
        }
        else {
            $("#property").prop('checked', true).change();
        }
        //$("#property").prop('checked', true).change();
    });
</script>