                <div class="col-md-9 text-center"> @*<input type="text" class="master-form-input" disabled />*@ Weekly Report </div>
                <div class="col-md-1 report-checkbox-align"> <input type="checkbox" class="input-checkbox" /> </div>
            </div>
        </div>
    </div>
    <div class="col-md-7 p-b-10">
        <div class="col-md-6">
            <div class="col-md-12 report-form-row">
                <div class="col-md-1"> 3. </div>
                <div class="col-md-9 text-center"> @*<input type="text" class="master-form-input" />*@ Tenant Form </div>
                <div class="col-md-1 report-checkbox-align"> <input type="checkbox" class="input-checkbox" /> </div>
