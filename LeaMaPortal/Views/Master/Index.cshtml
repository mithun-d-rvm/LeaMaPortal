@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row font-15">
    <div class="col-md-5">
        <div class="col-md-12 form-input-padding">
            <div class="col-md-4">
                Name:
            </div>
            <div class="col-md-8">
                <input type="text" class="master-form-input" />
            </div>
        </div>
        <div class="col-md-12 form-input-padding">
            <div class="col-md-4">
                User ID:
            </div>
            <div class="col-md-8">
                <input type="text" class="master-form-input" />
            </div>
        </div>
        <div class="col-md-12 form-input-padding">
            <div class="col-md-4">
                Password:
            </div>
            <div class="col-md-8">
                <input type="text" class="master-form-input" />
            </div>
        </div>
        <div class="col-md-12 form-input-padding">
            <div class="col-md-4">
                Confirm Password:
            </div>
            <div class="col-md-8">
                <input type="text" class="master-form-input" />
            </div>
        </div>
        <div class="col-md-12 form-input-padding">
            <div class="col-md-4">
                Enable:
            </div>
            <div class="col-md-8">
                <input type="text" class="master-form-input" />
            </div>
        </div>
        <div class="col-md-12 form-input-padding">
            <div class="col-md-4">
                &nbsp;
            </div>
            <div class="col-md-8">
                <div class="col-md-6">
                    <div class="col-md-1">
                        <input type="checkbox" class="input-checkbox">
                    </div>
                    <div class="col-md-8 m-t-3">
                        <span> Edit </span>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="col-md-1">
                        <input type="checkbox" class="input-checkbox">
                    </div>
                    <div class="col-md-8 m-t-3">
                        <span> Delete </span>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-md-12 form-input-padding">
            <div class="col-md-4">
                &nbsp;
            </div>
            <div class="col-md-4">
                <button class="btn master-form-btn">Submit</button>
            </div>
        </div>
    </div>
    <div class="col-md-7">
        <div class="col-md-6">
            <h5 class="p-b-25"> FORM DATA </h5>
        </div>
        <div class="col-md-6">
            <h5 class="p-b-25"> REPORT DATA </h5>
        </div>
    </div>
    <div class="col-md-7 p-b-10">
        <div class="col-md-6">
            <div class="col-md-12 report-form-row">
                <div class="col-md-1"> 1. </div>
                <div class="col-md-9 text-center"> @*<input type="text" class="master-form-input" />*@ Regional Form </div>                
                <div class="col-md-1 report-checkbox-align"> <input type="checkbox" class="input-checkbox" /> </div>
            </div>
        </div>
        <div class="col-md-6">
            <div class="col-md-12 report-form-row">
                <div class="col-md-1"> 1. </div>
                <div class="col-md-9 text-center"> @*<input type="text" class="master-form-input" disabled />*@ Daily Report </div>
                <div class="col-md-1 report-checkbox-align"> <input type="checkbox" class="input-checkbox" /> </div>
            </div>
        </div>
    </div>
    <div class="col-md-7 p-b-10">
        <div class="col-md-6">
            <div class="col-md-12 report-form-row">
                <div class="col-md-1"> 2. </div>
